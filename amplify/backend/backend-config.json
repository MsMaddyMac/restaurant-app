{
  "api": {
    "restaurantapp": {
      "service": "AppSync",
      "providerPlugin": "awscloudformation",
      "output": {
        "authConfig": {
          "defaultAuthentication": {
            "authenticationType": "API_KEY",
            "apiKeyConfig": {
              "apiKeyExpirationDays": 365,
              "description": "restaurant app key"
            }
          },
          "additionalAuthenticationProviders": []
        }
      }
    }
  },
  "function": {
    "newRestaurant": {
      "build": true,
      "providerPlugin": "awscloudformation",
      "service": "Lambda"
    }
  }
}